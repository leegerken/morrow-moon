[debug]

gdscript/warnings/untyped_declaration=1

[dialogue_manager]

general/balloon_path="res://systems/dialogue_system/dialog/balloon.tscn"
general/new_with_template=false

[display]

window/size/viewport_width=320
window/size/viewport_height=200
window/size/window_width_override=1440
window/size/window_height_override=900
window/subwindows/embed_subwindows=false
window/stretch/mode="viewport"
window/stretch/aspect="expand"
window/vsync/vsync_mode=0
mouse_cursor/custom_image="res://art/ui/mouse.png"
mouse_cursor/custom_image_hotspot=Vector2(7, 0)

[editor]

movie_writer/movie_file="/Users/lee/Movies/Demo.avi"

[editor_plugins]

enabled=PackedStringArray("res://addons/dialogue_manager/plugin.cfg")

[filesystem]

import/blender/enabled=false

